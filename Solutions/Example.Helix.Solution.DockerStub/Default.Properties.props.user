<Project>

  <PropertyGroup>
    <PublishRootPath>$(SolutionDir)Docker\</PublishRootPath>
    <PublishPathWebsite>$(PublishRootPath)\website\publish</PublishPathWebsite>
    <PublishPathXConnect>$(PublishRootPath)\xconnect</PublishPathXConnect>
    <PublishPathXCProcessing>$(PublishRootPath)\xconnect-processing</PublishPathXCProcessing>
    <PublishPathXCIndexWorker>$(PublishRootPath)\xconnect-indexworker\publish</PublishPathXCIndexWorker>
    <PublishPathXCAutomationEngine>$(PublishRootPath)\xconnect-automationengine\publish</PublishPathXCAutomationEngine>
    <PublishPathIdentity>$(PublishRootPath)\website\identity</PublishPathIdentity>
    <IsStandaloneBuild>$isStandAlone$</IsStandaloneBuild>
    <AutoPublishOnBuild>$autoPublishOnBuild$</AutoPublishOnBuild>
    <RunXmlTransforms>$runXmlTransformsOnPublish$</RunXmlTransforms>
    <XmlTransformFilePostfix>$transformFilePostfix$</XmlTransformFilePostfix>
  </PropertyGroup>
</Project>
