<templateManifest typeOfTemplate="Module">
  <name>Sitecore Host Context Client</name>
  <description>Sitecore Host Context / Client project for deploying referenced plugins</description>
  <version>1.0.0</version>
  <author>Anders Laub</author>
  <sourceFolder>/src</sourceFolder>
  <saveOnCreate>true</saveOnCreate>
  <templateEngine type="LaubPlusCo.Foundation.HelixTemplating.TemplateEngine.DefaultHelixTemplateEngine, LaubPlusCo.Foundation.HelixTemplating" />
  <replacementTokens>
    <token key="$moduleName$" displayName="Module name" default="$safeprojectname$" input="Text" required="true" validationType="LaubPlusCo.Foundation.HelixTemplating.Tokens.ValidateModuleNameToken, LaubPlusCo.Foundation.HelixTemplating" />
    <token key="$moduleNamespace$" displayName="Module namespace" required="true" default="Project.$safeprojectname$.Client" input="Text" suggestType="LaubPlusCo.Foundation.HelixTemplating.Tokens.SuggestNamespaceToken, LaubPlusCo.Foundation.HelixTemplating" validationType="LaubPlusCo.Foundation.HelixTemplating.Tokens.ValidateNamespaceToken, LaubPlusCo.Foundation.HelixTemplating" />
    <token key="$projectrootfolderName$" displayName="Project root folder name (f. ex. code, website, commerce, xconnect)" required="true" default="code" input="Text" />
    <token key="$netframeworkversion$" displayName="Target Framework" default="netstandard2.0" required="true" input="Text" />
    <token key="$targetEnvironment$" displayName="Environment" default="production" required="true" input="Text" />
    <token key="$publishTargetType$" displayName="Target type" input="Selection" required="true">
      <option name="identity" />
      <option name="universaltracker" />
    </token>
  </replacementTokens>
  <projectsToAttach>
    <projectFile path="/src/Project/$modulename$/$projectrootfolderName$/$moduleNamespace$.csproj" />
  </projectsToAttach>
  <ignoreFiles>
    <file path="/template.manifest.xml" />
  </ignoreFiles>
</templateManifest>